        "php": ">=8.0",
        "guzzlehttp/guzzle": "^7.0",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
